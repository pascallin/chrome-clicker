{
  "name": "Pascal HuaWei Scrapy Extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Get Mate 40 pro+",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "My Bookmarks",
      "default_icon": "images/taobao.png",
      "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}